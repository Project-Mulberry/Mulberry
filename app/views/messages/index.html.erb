<p id="notice"><%= notice %></p>

<h1>Listing Messages</h1>

<table>
  <thead>
  <tr>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.phone %></td>
      <td><%= user.name %></td>
      <td><%= link_to 'Show', message_path(user.uid) %></td>

    </tr>
  <% end %>
  </tbody>
</table>

<br>

